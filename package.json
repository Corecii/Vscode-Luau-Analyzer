{
	"name": "vscode-luau-analyzer",
	"displayName": "Vscode-Luau-Analyzer",
	"version": "1.0.1",
	"description": "Vscode port of zeux's SublimeLinter-Luau",
	"main": "./out/extension.js",
	"engines": {
		"vscode": "^1.65.0"
	},
	"activationEvents": [
		"onlanguage:luau",
		"onLanguage:lua"
	],
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/node": "14.x",
		"typescript": "^4.3.2"
	},
	"keywords": [
		"vscode",
		"luau",
		"lua"
	],
	"categories": [
		"Linters"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/HawDevelopment/VscodeLinter-Luau"
	},
	"author": "HawDevelopment",
	"publisher": "HawDevelopment",
	"license": "MIT"
}
